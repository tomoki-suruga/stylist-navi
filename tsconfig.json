  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "types": ["node"],
    "sourceMap": true,
    "outDir": "./js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "tsconfig.json"]
}
